{
  "name": "web-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cp ./.env.dev ./.env.local && next dev --turbopack",
    "dev:prod": "cp ./.env.prod ./.env.local && next dev --turbopack",
    "build": "next build",
    "build:mobile:dev": "TARGET_PLATFORM=mobile cp ./.env.dev ./.env.local && TARGET_PLATFORM=mobile next build && TARGET_PLATFORM=mobile npx cap sync",
    "build:mobile:prod": "TARGET_PLATFORM=mobile cp ./.env.prod ./.env.local && TARGET_PLATFORM=mobile next build && TARGET_PLATFORM=mobile npx cap sync",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build:staging": "cp ./.env.dev ./.env.local && next build",
    "build:production": "cp ./.env.prod ./.env.local && next build",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start",
    "ts": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:stripe": "jest --runInBand --verbose ./src/services/server/stripe/__tests__",
    "storybook": "storybook dev -p 6006 -c .storybook",
    "build-storybook": "storybook build -c .storybook",
    "prettier": "prettier --write \"./src/**/*.{js,json,ts,tsx,md,yml}\"",
    "prettier:js": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "codegen:client": "graphql-codegen --config codegen:client.ts",
    "codegen:server": "graphql-codegen --config codegen:server.ts",
    "codegen": "npm run codegen:client && npm run codegen:server",
    "sanity:schema": "npx sanity schema extract --path ./src/types/generated/sanity/schema.json",
    "sanity:types": "npx sanity typegen generate",
    "sanity:codegen": "npm run sanity:schema && npm run sanity:types",
    "supabase:migrate:staging": "env-cmd -f .env.dev -- sh -c 'npx supabase link --project-ref $NEXT_PUBLIC_SUPABASE_REF && npx supabase db push'",
    "supabase:migrate:prod": "env-cmd -f .env.prod -- sh -c 'npx supabase link --project-ref $NEXT_PUBLIC_SUPABASE_REF && npx supabase db push'"
  },
